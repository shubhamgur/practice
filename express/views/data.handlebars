<h1>Table From</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">age</th>
      <th scope="col">name</th>
      <th scope="col">address</th>
    </tr>
  </thead>
  {{#each lists}}
  <tbody>
    <tr >
      <th scope="row">{{this._id}}</th>
      <td>{{this.age}}</td>
      <td>{{this.name}}</td>
      <td>{{this.address}}</td>
    </tr>
  </tbody>
  {{/each}}
</table>